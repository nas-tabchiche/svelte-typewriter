{
  "name": "svelte-typewriter",
  "description": "A simple and reusable typewriter effect for your Svelte applications",
  "keywords": [
    "svelte",
    "typewriter-effect"
  ],
  "homepage": "https://sr.ht/~henriquehbr/svelte-typewriter",
  "repository": {
    "type": "git",
    "url": "https://git.sr.ht/~henriquehbr/svelte-typewriter"
  },
  "bugs": {
    "url": "https://todo.sr.ht/~henriquehbr/svelte-typewriter",
    "email": "henriqueborgeshbr@gmail.com"
  },
  "version": "3.1.3",
  "main": "Typewriter.svelte",
  "svelte": "Typewriter.svelte",
  "type": "module",
  "author": {
    "name": "Henrique Borges",
    "email": "henriqueborgeshbr@gmail.com"
  },
  "license": "MIT",
  "files": [
    "helpers",
    "modes",
    "*.js",
    "*.d.ts"
  ],
  "types": "index.d.ts",
  "publishConfig": {
    "directory": "package"
  },
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "husky install",
    "deploy-demo": "./deploy-demo",
    "prerelease": "docker rmi -f release && DOCKER_BUILDKIT=1 docker build --build-arg HOST_USER_UID=\"$(id -u)\" -t release .",
    "release": "docker run --rm -t -v $PWD:/repo -e GIT_NAME=\"$(git config user.name)\" -e GIT_EMAIL=\"$(git config user.email)\" release ./bump-package-json",
    "prettier": "prettier --write \"./src/**/*.js\"",
    "prepublish": "svelte-kit package"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "1.0.0-next.38",
    "@sveltejs/kit": "1.0.0-next.403",
    "husky": "^8.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte2tsx": "^0.5.13",
    "typescript": "^4.7.4",
    "vite": "^3.0.4"
  },
  "peerDependencies": {
    "svelte": ">=3.47.x"
  }
}
